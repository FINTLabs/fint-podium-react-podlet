{
  "name": "@fintlabs/fint-podium-react-podlet",
  "version": "1.0.0",
  "description": "This is a small helper library to help create a podlet for you react project to run in the FINTLabs/FLAIS environment.",
  "private": false,
  "main": "src/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "start-example": "npx tsc && cp src/example/asset-manifest.json dist/src/example && node dist/src/example/podlet-service.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/example/podlet-service.js\"",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "podium-lib",
    "podium",
    "fint",
    "fintlabs",
    "vigo-iks",
    "react"
  ],
  "homepage": "https://www.fintlabs.no",
  "author": "FINTLabs",
  "license": "MIT",
  "dependencies": {
    "@podium/podlet": "^4.4.69",
    "@types/express-prometheus-middleware": "^1.2.1",
    "@types/morgan": "^1.9.4",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "log4js": "^6.7.1",
    "morgan": "^1.10.0",
    "prom-client": "^14.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
