{
  "name": "@fintlabs/fint-podium-react-podlet",
  "version": "1.0.0",
  "description": "This is a small helper library to help create a podlet for you react project to run in the FINTLabs/FLAIS environment.",
  "private": false,
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "start-example": "npx tsc && cp example/asset-manifest.json dist/src/example && node dist/src/example/podlet-service.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/src/example/podlet-service.js\"",
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "podium-lib",
    "podium",
    "fint",
    "fintlabs",
    "vigo-iks",
    "react"
  ],
  "homepage": "https://www.fintlabs.no",
  "author": "FINTLabs",
  "license": "MIT",
  "dependencies": {
    "@podium/podlet": "^4.4.69",
    "@types/express-prometheus-middleware": "^1.2.1",
    "@types/morgan": "^1.9.4",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "log4js": "^6.7.1",
    "morgan": "^1.10.0",
    "prom-client": "^14.1.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@parcel/packager-ts": "^2.8.2",
    "@parcel/transformer-typescript-types": "^2.8.2",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.5.0",
    "concurrently": "^7.6.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "events": "^3.1.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.3.1",
    "lodash.camelcase": "^4.3.0",
    "nodemon": "^2.0.20",
    "os-browserify": "^0.3.0",
    "parcel": "^2.8.2",
    "path-browserify": "^1.0.0",
    "prettier": "^2.8.3",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "rimraf": "^4.0.6",
    "rollup": "^3.10.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.0",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4",
    "url": "^0.11.0",
    "util": "^0.12.3"
  }
}
